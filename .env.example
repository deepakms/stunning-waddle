# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# App Configuration
EXPO_PUBLIC_APP_URL=https://app.couplesworkout.com

# Error Monitoring (Sentry)
EXPO_PUBLIC_SENTRY_DSN=https://your-key@sentry.io/project-id

# Analytics (choose one or both)
EXPO_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token
EXPO_PUBLIC_AMPLITUDE_API_KEY=your-amplitude-api-key

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_MONITORING=true
